<% provide :title, t(".all-carts") %>
<%= render "admin/admin_menu" %>
<div class="col-md-8 " id="manager">
  <div class="col-xs-12" id="title_manager">
    <div class="col-xs-6">
      <h4><%= t(".all-carts") %></h4>
    </div>
  </div>
  <div class="col-xs-12 nopadding center-info">
    <table class="table table-striped table-hover">
      <tr>
        <th><%= t ".id" %></th>
        <th><%= t ".user_eamil" %></th>
        <th><%= t ".cart_price" %></th>
        <th></th>
      </tr>
       <%= render partial: "carts_list", collection: @carts, as: :c %>
    </table>
  </div>
  <div class="col-xs-12 nopadding">
    <%= paginate @carts, theme: 'twitter-bootstrap-3' %>
  </div>
</div>
